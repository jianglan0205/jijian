-decoration: none;\r\n}\r\nimg {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n}\r\n\r\n/* style */\r\nbody {\r\n  background: #eee;\r\n  background: url(\"./images/bizhi4.jpg\");\r\n  background-size: cover;\r\n}\r\n\r\n.globalHeader {\r\n  margin: 20px;\r\n  /* 居中,margin:20px 20px的简写,上下20px,左右20px */\r\n}\r\n@media (min-width: 500px) {\r\n  .globalHeader {\r\n    margin: 200px 0 60px 0;\r\n  }\r\n}\r\n.searchForm {\r\n  display: flex;\r\n  /* flex布局 */\r\n  justify-content: space-between;\r\n}\r\n@media (min-width: 500px) {\r\n  .searchForm {\r\n    max-width: 500px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n}\r\n/* 媒体查询,小于500px的不管,且在大于500px时,会和上面的.searchForm合并 */\r\n/* 当这个元素有最大宽度,或者宽度固定的时候,可以用margin来做居中,margin:0 auto不太好,0会覆盖别人的上下margin*/\r\n@media (max-width: 500px) {\r\n}\r\n/* 媒体查询,大于500px的不管 */\r\n.searchForm > input {\r\n  width: 100%;\r\n  margin-right: 10px;\r\n  /* input和搜索中间10px */\r\n  height: 40px;\r\n  padding: 0 10px;\r\n  /* 内部输入的内容,上下0px,左右10px */\r\n  border: 1px solid #ddd;\r\n  /* 加圆角的时候先把默认border覆盖掉,不然很奇怪 */\r\n  border-radius: 4px;\r\n  /* 加个4px的圆角 */\r\n}\r\n.searchForm > button {\r\n  white-space: nowrap;\r\n  /* 搜索两个字不换行 */\r\n  padding: 0 28px;\r\n  /* 让按钮自然点 */\r\n  border: none;\r\n  border-radius: 4px;\r\n  /* 同input里的,none掉或覆盖掉 */\r\n  background: #0282b9;\r\n  color: white;\r\n  font-size: 16px;\r\n  /* https://www.iconfont.cn/里直接拿到 */\r\n}\r\n\r\n.globalMain {\r\n  max-width: 900px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.siteList {\r\n  margin: 20px;\r\n  /* 上下左右各20px */\r\n  display: flex;\r\n  /* 变成内联 */\r\n  flex-wrap: wrap;\r\n  /* 大格子换行 */\r\n  justify-content: space-between;\r\n}\r\n@media (min-width: 500px) {\r\n  .siteList {\r\n    margin-left: 0;\r\n    margin-right: -25px;\r\n    justify-content: flex-start;\r\n    /* 覆盖space-between */\r\n  }\r\n}\r\n/* 干掉pc版的左右margin */\r\n.siteList > li {\r\n  margin-bottom: 10px;\r\n}\r\n@media (min-width: 500px) {\r\n  .siteList > li {\r\n    margin-right: 25px;\r\n  }\r\n}\r\n.siteList .site {\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  /* 字居中 */\r\n  align-items: center;\r\n  /* 字上下居中 */\r\n  flex-direction: column;\r\n  /* 字竖着显示 */\r\n  background: #eee;\r\n  border-radius: 4px;\r\n  /* 圆角 */\r\n  padding: 20px 0;\r\n  /* 上下间距 */\r\n  position: relative;\r\n  /* 给close做定位 */\r\n  cursor: pointer;\r\n  /* 鼠标变成手 */\r\n}\r\n.siteList .site > .logo {\r\n  width: 64px;\r\n  height: 64px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  /* A居中 */\r\n  font-size: 64px;\r\n  /* A大小 */\r\n  text-transform: uppercase;\r\n  /* 全大写 */\r\n}\r\n.siteList .site > .link {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n.siteList .site > .close {\r\n  position: absolute;\r\n  right: 6px;\r\n  top: 2px;\r\n  display: none;\r\n  cursor: default;\r\n}\r\n.siteList .site:hover > .close {\r\n  display: block;\r\n}\r\n/* 鼠标附上去显示X */\r\n.siteList .addButton {\r\n  border: 1px solid #ddd;\r\n  background: white;\r\n  width: 160px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  /* 三句话居中 */\r\n  flex-direction: column;\r\n  /* 变成上下结构 */\r\n  padding: 20px 0;\r\n}\r\n.siteList .addButton .icon {\r\n  width: 64px;\r\n  height: 64px;\r\n}\r\n.siteList .addButton .text {\r\n  font-size: 14px;\r\n  margin-top: 4px;\r\n}\r\n.siteList .addButton .icon-wrapper {\r\n  width: 64px;\r\n  height: 64px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  /* 三句话+居中 */\r\n}\r\n"],"names":[],"version":3,"file":"index.HASH_REF_f0918a547ace1733ebc251b0de60a1d7.css.map"}